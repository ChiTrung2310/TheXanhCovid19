
@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}


<h4 style="text-align:center">Thêm mới dữ liệu người dùng từ file Excel</h4>
<hr />
@using (Html.BeginForm("UploadExcel", "Excel", FormMethod.Post, new { enctype = "multipart/form-data", onsubmit = "return myFunction()" }))
{
<div style="text-align:center; padding:10px">
    <div class="form-horizontal">

        <div class="form-group">
            <img width="600px" height="300px" src="/Assets/admin/img/ExceltoSQL.jpeg" alt="...">
        </div>

        <div class="form-group">
            <div class="control-label col-md-10" style="padding:30px">Nhập File Excel:</div>
            <div class="col-md-12">
                <input type="file" id="FileUpload" name="FileUpload" class="" />
            </div>
        </div>


        <div class="form-group">
            <div class="col-md-offset-2 col-md-12" style="padding:30px">
                <input type="submit" value="Thêm mới" id="btnSubmit" class="btn btn-primary" />
            </div>
        </div>
    </div>

</div>
}